<h1>Welcome to Blocmetrics</h1>
<h3>Track activity on your apps</h3>

<div class="row">
  <div class="col-md-8">
    <% @users.each do |user| %>
      <div class="media">
        <div class="media-body">
          <div class="media-heading">
            <p><%= user.email %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
