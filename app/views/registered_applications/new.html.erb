<h1>Register New App</h1>
<div class="row">
  <div class="col-md-8">
    <%= form_for @registered_application do |f| %>
    <div class="form-group">
      <%= f.label :url %>
      <%= f.text_field :url %>
    </div>
    <div class="form-group">
      <%= f.submit "Save" %>
    </div>
    <% end %>
  </div>
</div>
